<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Accordion?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="350.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.studentattendacneproject.Controllers.AdminMainController">
   <children>
      <Accordion layoutX="90.0" layoutY="95.0" />
      <TabPane fx:id="adminMainPane" prefHeight="400.0" prefWidth="350.0" tabClosingPolicy="UNAVAILABLE">
        <tabs>
          <Tab closable="false" text="Accounts">
            <content>
              <AnchorPane fx:id="accountsPane" minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                     <children>
                        <Button fx:id="butLogOut" layoutX="277.0" layoutY="332.0" mnemonicParsing="false" onAction="#onLogOut" text="Log Out" AnchorPane.bottomAnchor="14.0" AnchorPane.rightAnchor="14.0">
                           <font>
                              <Font name="Times New Roman" size="12.0" />
                           </font>
                        </Button>
                        <Button fx:id="butCreateAccount" layoutX="61.0" layoutY="64.0" mnemonicParsing="false" onAction="#onCreateAccount" prefHeight="35.0" prefWidth="229.0" text="Create Account" AnchorPane.leftAnchor="61.0" AnchorPane.rightAnchor="60.0">
                           <font>
                              <Font name="Times New Roman" size="20.0" />
                           </font>
                        </Button>
                        <Button fx:id="butEditAccount" layoutX="61.0" layoutY="129.0" mnemonicParsing="false" onAction="#onEditAccount" prefHeight="35.0" prefWidth="229.0" text="Edit Account" AnchorPane.leftAnchor="61.0" AnchorPane.rightAnchor="60.0">
                           <font>
                              <Font name="Times New Roman" size="20.0" />
                           </font>
                        </Button>
                        <Button fx:id="butDeleteAccount" layoutX="61.0" layoutY="192.0" mnemonicParsing="false" onAction="#onDeleteAccount" prefHeight="35.0" prefWidth="229.0" text="Delete Account" AnchorPane.leftAnchor="61.0" AnchorPane.rightAnchor="60.0">
                           <font>
                              <Font name="Times New Roman" size="20.0" />
                           </font>
                        </Button>
                        <Button fx:id="butShowAccounts" layoutX="61.0" layoutY="254.0" mnemonicParsing="false" onAction="#onShowAccounts" prefHeight="35.0" prefWidth="229.0" text="Show Accounts">
                           <font>
                              <Font name="Times New Roman" size="20.0" />
                           </font>
                        </Button>
                     </children>
                  </AnchorPane>
            </content>
          </Tab>
          <Tab closable="false" text="Courses">
            <content>
              <AnchorPane fx:id="coursesPane" minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                     <children>
                        <Button fx:id="butLogOut1" layoutX="281.0" layoutY="337.0" mnemonicParsing="false" onAction="#onLogOut" text="Log Out" AnchorPane.bottomAnchor="12.0" AnchorPane.rightAnchor="12.0">
                           <font>
                              <Font name="Times New Roman" size="12.0" />
                           </font>
                        </Button>
                        <Button fx:id="butCreateCourse" layoutX="61.0" layoutY="42.0" mnemonicParsing="false" onAction="#onCreateCourse" prefHeight="35.0" prefWidth="229.0" text="Create Course">
                           <font>
                              <Font name="Times New Roman" size="20.0" />
                           </font>
                        </Button>
                        <Button fx:id="butEditCourse" layoutX="61.0" layoutY="89.0" mnemonicParsing="false" onAction="#onEditCourse" prefHeight="35.0" prefWidth="229.0" text="Edit Course">
                           <font>
                              <Font name="Times New Roman" size="20.0" />
                           </font>
                        </Button>
                        <Button fx:id="butDeleteCourse" layoutX="61.0" layoutY="136.0" mnemonicParsing="false" onAction="#onDeleteCourse" prefHeight="35.0" prefWidth="229.0" text="Delete Course">
                           <font>
                              <Font name="Times New Roman" size="20.0" />
                           </font>
                        </Button>
                        <Button fx:id="butShowCourses" layoutX="61.0" layoutY="184.0" mnemonicParsing="false" onAction="#onShowCourses" prefHeight="35.0" prefWidth="229.0" text="Show Courses">
                           <font>
                              <Font name="Times New Roman" size="20.0" />
                           </font>
                        </Button>
                        <Button fx:id="butShowStudents" layoutX="61.0" layoutY="231.0" mnemonicParsing="false" onAction="#onShowStudents" prefHeight="35.0" prefWidth="229.0" text="Show Students">
                           <font>
                              <Font name="Times New Roman" size="20.0" />
                           </font>
                        </Button>
                        <Button fx:id="butShowStudents1" layoutX="61.0" layoutY="278.0" mnemonicParsing="false" onAction="#onSearchAbsentStudents" prefHeight="35.0" prefWidth="229.0" text="Show Absent Students">
                           <font>
                              <Font name="Times New Roman" size="20.0" />
                           </font>
                        </Button>
                     </children>
                  </AnchorPane>
            </content>
          </Tab>
        </tabs>
      </TabPane>
   </children>
</AnchorPane>
